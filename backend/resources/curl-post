-H "Content-Type: multipart/form-data" \
-F "data={\"patientId\":1,\"bodyPart\":\"Hand\",\"uploadDate\":\"2023-10-01\",\"imagePath\":\"wololo\"};type=application/json" \
-F "file=@.../Xray_app/backend/resources/3.jpeg"

